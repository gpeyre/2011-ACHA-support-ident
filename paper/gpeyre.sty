%% pour faire une figure
\newcommand{\myfigure}[3]{
    \begin{figure}[ht] %
   % \preimagespace %
    \begin{center} %
        #1                          % usual include graphics
    \end{center} %
    % \vspace{-4mm}
        \caption{\textit{#2}}       % caption
    \label{#3}          % label
    % \postimagespace %
    \end{figure} %
}

\newcommand{\myfigurespace}[4]{
    \begin{figure}[ht]
    \preimagespace
    \begin{center}
        #1                          % usual include graphics
    \end{center}
    \vspace{-4mm}
        \caption{\textit{#2}}       % caption
    \label{#3}          % label
    \vspace{#4mm}
    \end{figure}
}

\newcommand{\myfigurestar}[3]{
    \begin{figure*}[ht]
    \begin{center}
        #1                          % usual include graphics
    \end{center}
    \vspace{-4mm}
        \caption{\textit{#2}}       % caption
    \label{#3}          % label
    \postimagespace
    \end{figure*}
}

\newcommand{\myfigurestarspace}[4]{
    \begin{figure*}[ht]
    \begin{center}
        #1                          % usual include graphics
    \end{center}
    \vspace{-4mm}
        \caption{\textit{#2}}       % caption
    \label{#3}          % label
    \vspace{#4mm}
    \end{figure*}
}

