}}{\end{list}}

\newenvironment{rn}[1]{\begin{list}{}{
 \topsep=0.5ex \itemsep=0.3ex \labelsep=0.3em \parsep=0ex
 \settowidth{\labelwidth}{(#1)}\listparindent=1em
 \leftmargin=\labelwidth \addtolength{\leftmargin}{2\labelsep}
}}{\end{list}}


